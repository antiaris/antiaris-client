{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx":true
        }
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "System": true
    },
    "rules": {
        "no-unused-vars": 0,
        "camelcase": 2,
        "semi": [2, "always"],
        "no-var": 2,
        "curly": 2,
        "eqeqeq": 2,
        "no-alert": 2,
        "no-eval": 2,
        "no-loop-func": 2,
        "no-proto": 2,
        "no-unused-expressions": 2,
        "no-with": 2,
        "vars-on-top": 2,
        "no-useless-call": 2,
        "quotes": [2, "single"],
        "no-sequences": 2,
        "no-process-env": 2,
        "no-labels": 2,
        "no-invalid-this": 2,
        "default-case": 2
    }
}